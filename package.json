{
  "name": "spelling",
  "version": "0.0.1",
  "description": "A spelling game created using Redux and React",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
    "test:coverage": "jest --no-cache --coverage",
    "test:watch": "npm test -- --watch",
    "testMocha": "NODE_ENV=test mocha --recursive --compilers js:babel-core/register --require ./test/setup.js",
    "lint": "eslint js --cache"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulbevis/spelling.git"
  },
  "author": "Paul Bevis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paulbevis/spelling/issues"
  },
  "homepage": "https://github.com/paulbevis/spelling#readme",
  "dependencies": {
    "material-ui": "^0.15.2",
    "ramda": "^0.21.0",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-motion": "^0.4.4",
    "react-redux": "^4.4.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^0.1.0"
  },
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-jest": "12.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "1.1.1",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-jest": "11.0.2",
    "babel-preset-react": "6.11.1",
    "deep-freeze": "0.0.1",
    "eslint": "2.13.1",
    "eslint-config-standard": "5.3.5",
    "eslint-loader": "1.4.1",
    "eslint-plugin-react": "5.2.2",
    "eslint-plugin-standard": "1.3.3",
    "eslint-plugin-promise": "1.0.8",
    "expect": "1.20.2",
    "express": "4.14.0",
    "jest-cli": "12.1.1",
    "jsdom": "^5.6.1",
    "material-design-icons": "^2.2.3",
    "mocha": "^2.4.5",
    "node-libs-browser": "^0.5.2",
    "react-addons-test-utils": "15.2.1",
    "webpack": "^1.12.14"
  }
}
